<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.training.EstimationOfHealth">

    <TextView
        android:id="@+id/text_health"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="41dp"
        android:text="Оцените свое самочувстие" />

    <ListView
        android:id="@+id/listView"
        android:layout_width="match_parent"
        android:layout_height="473dp"
        android:layout_below="@+id/text_health"
        android:layout_marginTop="16dp"></ListView>

    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/estimBtnBack"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="11dp"
        android:text="Вернуться" />

</RelativeLayout>